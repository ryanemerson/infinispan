<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.5" name="org.jboss.as.security">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <exports>
        <exclude path="org/jboss/as/security/logging"/>
    </exports>

    <resources>
        <artifact name="${org.wildfly:wildfly-security}"/>
    </resources>

    <dependencies>
        <module name="javax.api"/>
        <module name="javax.security.jacc.api"/>
        <module name="javax.servlet.api"/>
        <module name="javax.transaction.api"/>
        <module name="org.apache.commons.cli" optional="true"/>
        <module name="org.jboss.as.core-security"/>
        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.domain-management"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.as.naming"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.metadata.common"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.remoting"/>
        <module name="org.jboss.security.negotiation"/>
        <module name="org.jboss.staxmapper"/>
        <module name="org.picketbox"/>
        <module name="org.wildfly.security.elytron"/>
        <module name="sun.jdk"/>
    </dependencies>
</module>
